<?xml version="1.0" encoding="utf-8"?>
<resources>

    <string-array name="brightness_keyword">
        <item>亮度</item>
        <item>屏幕</item>
    </string-array>
    <string-array name="brightness_down_regex">
        <item>.*[调|降|减].*[低|小|暗].*</item>
    </string-array>
    <string-array name="brightness_up_regex">
        <item>.*[调|升|增|提].*[高|大|亮].*</item>
    </string-array>
    <string-array name="brightness_limit_keyword">
        <item>最</item>
        <item>极</item>
    </string-array>

    <string-array name="launch_app_regex">
        <item>打开(.{2,})</item>
        <item>运行(.{2,})</item>
        <item>启动(.{2,})</item>
    </string-array>
    <string-array name="launch_exit_keyword">
        <item>退出当前应用</item>
        <item>关闭当前应用</item>
        <item>回到主界面</item>
    </string-array>

    <string-array name="launch_camera_image_regex">
        <item>拍.?.?照</item>
        <item>照.?.?相</item>
    </string-array>
    <string-array name="launch_camera_video_regex">
        <item>[录|摄].?.?[像|影]</item>
    </string-array>

    <string-array name="launch_music_regex">
        <item>[播|放|唱].*(?:音乐|歌曲|歌|曲)(.*)</item>
    </string-array>

    <string-array name="phone_regex">
        <item>.*(?:打个|打|回个|回)电话.*</item>
        <item>.*(?:拨|拨打|拨个)(?:号码|电话).*</item>
    </string-array>

    <string-array name="voice_volume_keyword">
        <item>声音</item>
        <item>音量</item>
    </string-array>
    <string-array name="voice_type_keyword">
        <item>系统</item>
        <item>音乐</item>
        <item>铃声</item>
        <item>通话</item>
        <item>闹钟</item>
        <item>通知</item>
    </string-array>
    <string-array name="voice_volume_down_regex">
        <item>.*[调|降|减][低|小|轻].*(?:音量|声音).*</item>
        <item>.*(?:音量|声音).*[调|降|减]?.*[低|小|轻].*</item>
    </string-array>
    <string-array name="voice_volume_up_regex">
        <item>.*[调|提|升|增][高|大|响].*(?:音量|声音).*</item>
        <item>.*(?:音量|声音).*[调|提|升|增]?.*[高|大|响].*</item>
    </string-array>
    <string-array name="voice_volume_limit_keyword">
        <item>最</item>
        <item>极</item>
    </string-array>
