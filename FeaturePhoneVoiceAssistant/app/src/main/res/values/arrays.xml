        <item>.+(?:等于|是)(?:多少|几).+</item>
    </string-array>

    <string-array name="calculator_regex">
        <item>.*(?:\\+|-|×|÷|开方|平方|次方|根号).*(?:等于|是)(?:多少|几)</item>
    </string-array>

    <string-array name="translate_regex">
        <item>(.{0,4})翻译.+</item>
    </string-array>

    <string-array name="weather_keyword">
        <item>天气</item>
        <item>气候</item>
        <item>气温</item>
        <item>温度</item>
        <item>冷不冷</item>
        <item>热不热</item>
        <item>感冒指数</item>
        <item>穿衣指数</item>
        <item>紫外线强度</item>
    </string-array>
    <string-array name="weather_regex">
        <item>.*(?:有|下|刮)(?:雨|雪|霜|风|雾|云|太阳).*</item>
        <item>.*(?:热|冷|户外运动|容易感冒|洗车|旅游|外出|出门)[吗|么]</item>
        <item>.*穿.+衣服.*</item>
    </string-array>

    <string-array name="couplet_keyword">
        <item>对联</item>
        <item>春联</item>
        <item>门联</item>
    </string-array>
    <string-array name="couplet_regex">
        <item>.*(?:根据|用)(.*)[写|做].*的?[对|春|门]联</item>
        <item>.*[写|做].*关于(.*)的?[对|春|门]联</item>
    </string-array>

    <string-array name="poem_keyword">
        <item>诗</item>
    </string-array>
    <string-array name="poem_regex">
        <item>.*(?:根据|用)(.*)[写|做].*的?诗</item>
        <item>.*[写|做].*关于(.*)的?诗</item>
    </string-array>

    <string-array name="joke_keyword">
        <item>笑话</item>
        <item>段子</item>
    </string-array>
    <string-array name="joke_regex">
