        <item>垃圾</item>
    </string-array>
    <string-array name="garbage_regex">
        <item>.*(?:啥|哪些|属于|是|什么).*(?:垃圾).*</item>
        <item>.*(?:垃圾).*(?:什么|哪些|啥).*</item>
    </string-array>

